{
  "name": "oddle-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "materialize-css": "^1.0.0-rc.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-js-pagination": "^3.0.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-transition-group": "^2.5.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sweetalert": "^2.1.2"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/assets/styles --include-path ./node_modules src/ -o src/assets/styles/",
    "watch-css": "npm run build-css && node-sass --include-path ./src/assets/styles --include-path ./node_modules src/ -o src/assets/styles/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5"
  }
}
